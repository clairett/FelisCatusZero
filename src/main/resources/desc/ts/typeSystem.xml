<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>Japanese Text Analyzer Type System</name>
  <description>Japanese text analyzer's type system</description>
  <version>1.0</version>
  <vendor>K. Sakamoto</vendor>
  <types>
    <typeDescription>
      <name>jeqa.types.TextAnnotation</name>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>id</name>
          <description>Textが持っているID</description>
          <rangeTypeName>uima.cas.Long</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>text</name>
          <description>Textが持っているテキスト</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>docno</name>
          <description>DOCNO</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>title</name>
          <description>title</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>scoreList</name>
          <description>スコアのリスト。インデックスがスコアラと対応する</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>jeqa.types.Score</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>morphemeList</name>
          <description>形態素のリスト</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>jeqa.types.ja.Morpheme</elementType>
        </featureDescription>
        <featureDescription>
          <name>contentWordList</name>
          <description>内容語リスト</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>beginTime</name>
          <description>開始時間</description>
          <rangeTypeName>jeqa.types.Time</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endTime</name>
          <description>終了時間</description>
          <rangeTypeName>jeqa.types.Time</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>geography</name>
          <description>geography</description>
          <rangeTypeName>jeqa.types.Geography</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>semanticTypeList</name>
          <description>semantic type list</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>jeqa.types.ja.SemanticType</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>jeqa.types.Geography</name>
      <description>地域</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>termList</name>
          <description>term list</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>area</name>
          <description>area</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>jeqa.types.Time</name>
      <description>時間</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>textList</name>
          <description>texts</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>year</name>
          <description>year</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>jeqa.types.Score</name>
      <description>スコア</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description>スコア</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>scorer</name>
          <description>スコアラ</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>jeqa.types.Exam</name>
      <description>試験問題</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>questionSet</name>
          <description>Examが出題するQuestionのセット</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>jeqa.types.Question</elementType>
        </featureDescription>
        <featureDescription>
          <name>label</name>
          <description>Examにつけられたユニークなラベル(実際にはExamのファイル名が入る)</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>dir</name>
          <description>Examのファイルが保持されているディレクトリへのパス</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lang</name>
          <description>言語</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>jeqa.types.Question</name>
      <description>Examで出題されたQuestion</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>xml</name>
          <description>XML Answer Section</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>document</name>
          <description>問題文</description>
          <rangeTypeName>jeqa.types.Document</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>label</name>
          <description>個々のQuestionにつけられたユニークなラベル</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>questionFormatType</name>
          <description>Question Format Type</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answerFormatType</name>
          <description>Answer Format Type</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>querySet</name>
          <description>QuestionがIRに投げたいQueryのセット</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>jeqa.types.Query</elementType>
        </featureDescription>
        <featureDescription>
          <name>keywordSet</name>
          <description>キーワード集合</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>jeqa.types.Keyword</elementType>
        </featureDescription>
        <featureDescription>
          <name>beginCharacterLimit</name>
          <description>何文字以上</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endCharacterLimit</name>
          <description>何文字以下</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>beginTimeLimit</name>
          <description>begin time limit</description>
          <rangeTypeName>jeqa.types.Time</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>endTimeLimit</name>
          <description>end time limit</description>
          <rangeTypeName>jeqa.types.Time</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>geographyLimit</name>
          <description>geography limit</description>
          <rangeTypeName>jeqa.types.Geography</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>questionFocusSet</name>
          <description>question focus set</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lexicalAnswerTypeSet</name>
          <description>lexical answer type set</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>semanticAnswerTypeSet</name>
          <description>semantic answer type set</description>
          <rangeTypeName>uima.cas.StringList</rangeTypeName>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
        <featureDescription>
          <name>answerSet</name>
          <description>回答のセット</description>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>jeqa.types.Answer</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>jeqa.types.Answer</name>
      <description>回答</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>document</name>
